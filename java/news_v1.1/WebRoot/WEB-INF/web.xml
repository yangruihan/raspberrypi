    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>toLogin</servlet-name>
    <servlet-class>com.yrh.web.ToLoginServlet</servlet-class>
