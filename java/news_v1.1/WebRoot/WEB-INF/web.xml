    <servlet-name>toError</servlet-name>
    <servlet-class>com.yrh.web.ToErrorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>toError</servlet-name>
    <url-pattern>/toError</url-pattern>
  </servlet-mapping>
  <servlet>
